/ {
	i2c@7000c000 {
		icm20628@68 {
			status = "disabled";
		};

		ak8963c@0c {
			status = "disabled";
		};

		jsa1127@39 {
			status = "disabled";
		};

		rt5658: rt5659.0-001a@1a {
                        compatible = "realtek,rt5658";
                        reg = <0x1a>;
                };
	};

	spi@7000d600 {
		status = "disabled";
	};

	/* extended sound node for superIO */
	sound {
		nvidia,dai-link-1 {
                        codec-dai = <&rt5658>;
                };
	};
};
