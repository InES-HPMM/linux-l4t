DTC_FLAGS := -i $(srctree)/arch/arm/boot/dts

DTCCPP_FLAGS := -I$(srctree)/arch/arm/boot/dts

DTB_NAMES := $(subst $\",,$(CONFIG_BUILD_ARM64_APPENDED_DTB_IMAGE_NAMES))
ifneq ($(DTB_NAMES),)
